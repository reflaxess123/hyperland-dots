{
  "log": {
    "level": "debug",
    "timestamp": true,
    "output": "/home/YOUR_USERNAME/.local/share/singbox-traffic.log"
  },
  "dns": {
    "servers": [
      {
        "tag": "google",
        "type": "https",
        "server": "8.8.8.8",
        "server_port": 443
      },
      {
        "tag": "local",
        "type": "udp",
        "server": "77.88.8.8",
        "server_port": 53
      }
    ],
    "rules": [
      {
        "domain": [
          "dns-shop.ru",
          "rubygems.org"
        ],
        "domain_suffix": [
          ".dns-shop.ru",
          ".rubygems.org"
        ],
        "action": "route",
        "server": "local"
      },
      {
        "domain_suffix": [
          ".ru"
        ],
        "action": "route",
        "server": "local"
      }
    ],
    "final": "google"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "sing-tun",
      "address": ["172.19.0.1/30"],
      "mtu": 1500,
      "auto_route": true,
      "strict_route": false,
      "stack": "mixed",
      "sniff": true,
      "sniff_override_destination": false
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "127.0.0.1",
      "listen_port": 1080,
      "sniff": true,
      "sniff_override_destination": false
    }
  ],
  "outbounds": [
    {
      "type": "vless",
      "tag": "proxy",
      "server": "YOUR_SERVER_IP",
      "server_port": 443,
      "uuid": "YOUR_UUID_HERE",
      "tls": {
        "enabled": true,
        "server_name": "google.com",
        "utls": {
          "enabled": true,
          "fingerprint": "chrome"
        },
        "reality": {
          "enabled": true,
          "public_key": "YOUR_REALITY_PUBLIC_KEY",
          "short_id": "YOUR_SHORT_ID"
        }
      }
    },
    {
      "type": "direct",
      "tag": "direct"
    }
  ],
  "route": {
    "default_domain_resolver": {
      "server": "google",
      "rewrite_ttl": 60
    },
    "rules": [
      {
        "protocol": "dns",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "action": "route",
        "outbound": "direct"
      },
      {
        "domain": [
          "dns-shop.ru",
          "rubygems.org",
          "avito.st",
          "avito.ru",
          "hcaptcha.com",
          "geetest.com",
          "yastatic.net",
          "bdimg.com"
        ],
        "domain_suffix": [
          ".dns-shop.ru",
          ".rubygems.org",
          ".avito.st",
          ".avito.ru",
          ".hcaptcha.com",
          ".geetest.com",
          ".yastatic.net",
          ".bdimg.com"
        ],
        "action": "route",
        "outbound": "direct"
      },
      {
        "domain_suffix": [
          ".ru"
        ],
        "action": "route",
        "outbound": "direct"
      },
      {
        "domain_regex": [
          "^[^.]+\\.ru$"
        ],
        "action": "route",
        "outbound": "direct"
      }
    ],
    "final": "proxy",
    "auto_detect_interface": true
  }
}
